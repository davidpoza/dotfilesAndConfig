[
    {
        "name": "Window Navigation",
        "shortcuts": [
            {
                "key": "Alt + Tab",
                "description": "Switches between windows"
            },
            {
                "key": "Super",
                "description": "Enter overview"
            },
            {
                "key": "Super + Left/Right",
                "description": "Tile windows to the left or right"
            },
            {
                "key": "Super + Up",
                "description": "Maximize window"
            },
            {
                "key": "Alt + F4",
                "description": "Close window"
            },
            {
                "key": "Super + Mouse Drag",
                "description": "Move window"
            },
            {
                "key": "F11",
                "description": "Make full screen/restore"
            },
            {
                "key": "Super + H",
                "description": "Minimize window"
            },
            {
                "key": "Super + Page Down",
                "description": "Switch to next workspace"
            },
            {
                "key": "Super + Page Up",
                "description": "Switch to previous workspace"
            },
            {
                "key": "Super + Home",
                "description": "Switch to first workspace"
            },
            {
                "key": "Super + End",
                "description": "Switch to last workspace"
            },
            {
                "key": "Super + Tab",
                "description": "Switch to next application"
            }
        ]
    },
    {
        "name": "Run and Close Applications",
        "shortcuts": [
            {
                "key": "Alt + F2",
                "description": "Run dialog"
            },
            {
                "key": "Super + 1,2,3,...",
                "description": "Run app by position in the launcher"
            },
            {
                "key": "Super + T",
                "description": "Open terminal"
            },
            {
                "key": "Super + Q",
                "description": "Quit application"
            }
        ]
    },
    {
        "name": "System Shortcuts",
        "shortcuts": [
            {
                "key": "Super + Space",
                "description": "Switch keyboard layout"
            },
            {
                "key": "Ctrl + Alt + Del",
                "description": "Opens the shutdown menu"
            },
            {
                "key": "Super + L",
                "description": "Locks the screen"
            },
            {
                "key": "Alt + F2, r",
                "description": "Restart GNOME Shell"
            },
            {
                "key": "Alt + Super + 8",
                "description": "Toggle zoom"
            },
            {
                "key": "Alt + Super + S",
                "description": "Toggle screen reader"
            }
        ]
    },
    {
        "name": "Application Shortcuts",
        "shortcuts": [
            {
                "key": "Ctrl + W",
                "description": "Close tab in tab enabled apps"
            },
            {
                "key": "Ctrl + T",
                "description": "Opens new tab in tab enabled apps"
            },
            {
                "key": "Ctrl + Shift + T",
                "description": "Reopen last closed tab in web browser"
            },
            {
                "key": "F5",
                "description": "Evince (document viewer) presentation mode)"
            },
            {
                "key": "F9",
                "description": "LibreOffice presentation mode"
            }
        ]
    },
    {
        "name": "Screenshot Shortcuts",
        "shortcuts": [
            {
                "key": "Alt + Print",
                "description": "Take a screenshot"
            },
            {
                "key": "Shift + Print",
                "description": "Take a screenshot of a region"
            },
            {
                "key": "Ctrl + Print",
                "description": "Take a screenshot and paste in a clipboard"
            },
            {
                "key": "Ctrl + Alt + Shift + R",
                "description": "Video capture desktop"
            }
        ]
    }
